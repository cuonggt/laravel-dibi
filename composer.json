{
    "name": "cuonggt/laravel-dibi",
    "description": "A wonderful GUI database management tool for Laravel.",
    "keywords": [
        "laravel",
        "database",
        "management"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Cuong Giang",
            "email": "thaicuong.giang@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/contracts": "5.6.*|5.7.*|5.8.*|6.*",
        "illuminate/database": "5.6.*|5.7.*|5.8.*|6.*",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|6.*",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "orchestra/database": "3.7.*",
        "orchestra/testbench": "3.7.*",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Cuonggt\\Dibi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cuonggt\\Dibi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cuonggt\\Dibi\\DibiServiceProvider"
            ],
            "aliases": {
                "Dibi": "Cuonggt\\Dibi\\Dibi"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
